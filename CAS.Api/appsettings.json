{
  "AWS": {
    "Region": "eu-west-2"
  },
  "ExampleQueueConsumer": {
    "QueueUrl" : "http://sqs.eu-west-2.127.0.0.1:4566/000000000000/example-queue",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 1,
    "Disabled": true
  },
  "SecondQueueConsumer": {
    "QueueUrl" : "http://sqs.eu-west-2.127.0.0.1:4566/000000000000/second-queue",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 1,
    "Disabled": true
  },
  "DisabledQueueConsumer": {
    "QueueUrl" : "http://sqs.eu-west-2.127.0.0.1:4566/000000000000/disabled-consumer-queue",
    "MaxNumberOfMessages": 10,
    "WaitTimeSeconds": 1,
    "Disabled": true
  },
  "ExampleQueuePublisher": {
    "TopicArn" : "arn:aws:sns:eu-west-2:000000000000:example-publisher-queue"
  },
  "Mongo": {
    "DatabaseUri": "mongodb://set-automatically-when-deployed/admin?authSource=$external&authMechanism=MONGODB-AWS",
    "DatabaseName": "central-animal-data-store"
  },
  "AllowedHosts": "*",
  "TraceHeader": "x-cdp-request-id",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
        }
      }
    ]
  }
}
